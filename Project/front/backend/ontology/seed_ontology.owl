    </ObjectPropertyRange>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#hasSection</IRI>
        <Literal>has section</Literal>
    </AnnotationAssertion>

    <Declaration>
