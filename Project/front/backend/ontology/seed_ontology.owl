    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#belongsToCourse"/>
        <Class IRI="#Course"/>
    </ObjectPropertyRange>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#belongsToCourse</IRI>
        <Literal>belongs to course</Literal>
    </AnnotationAssertion>

    <Declaration>
        <ObjectProperty IRI="#hasLesson"/>
    </Declaration>
    <InverseObjectProperties>
        <ObjectProperty IRI="#hasLesson"/>
        <ObjectProperty IRI="#belongsToCourse"/>
    </InverseObjectProperties>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#hasLesson</IRI>
        <Literal>has lesson</Literal>
    </AnnotationAssertion>

    <Declaration>
        <ObjectProperty IRI="#hasSection"/>
    </Declaration>
    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasSection"/>
        <Class IRI="#Lesson"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#hasSection"/>
