    <!-- Knowledge Relationships (between Learning Objects) -->
    <Declaration>
        <ObjectProperty IRI="#isPrerequisiteFor"/>
    </Declaration>
    <ObjectPropertyDomain>
        <ObjectProperty IRI="#isPrerequisiteFor"/>
        <Class IRI="#LearningObject"/>
    </ObjectPropertyDomain>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#isPrerequisiteFor"/>
        <Class IRI="#LearningObject"/>
    </ObjectPropertyRange>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#isPrerequisiteFor</IRI>
        <Literal>is prerequisite for</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#isPrerequisiteFor</IRI>
