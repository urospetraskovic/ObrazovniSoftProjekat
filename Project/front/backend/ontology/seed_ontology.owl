<?xml version="1.0"?>
<Ontology xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://example.org/solo-education-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     ontologyIRI="http://example.org/solo-education-ontology">
    
    <!-- ================================================================== -->
    <!-- SOLO TAXONOMY EDUCATIONAL ONTOLOGY - SEED VERSION                  -->
    <!-- ================================================================== -->
    <!-- 
    This is a SEED ONTOLOGY for the SOLO Quiz Generator system.
    It provides foundational classes and relationships that will be 
    populated and evolved as the system processes educational content.
    
    Based on:
    - SOLO Taxonomy (Biggs & Collis, 1982)
    - IEEE Learning Object Metadata (LOM) concepts
    - Bloom's Taxonomy alignment
    - Educational Assessment standards
    
    Author: SOLO Quiz Generator Project
    Version: 1.0 (Seed)
    Date: 2024
    -->

    <!-- Prefixes -->
    <Prefix name="" IRI="http://example.org/solo-education-ontology#"/>
    <Prefix name="owl" IRI="http://www.w3.org/2002/07/owl#"/>
    <Prefix name="rdf" IRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <Prefix name="rdfs" IRI="http://www.w3.org/2000/01/rdf-schema#"/>
    <Prefix name="xsd" IRI="http://www.w3.org/2001/XMLSchema#"/>
    <Prefix name="dc" IRI="http://purl.org/dc/elements/1.1/"/>
    <Prefix name="lom" IRI="http://ltsc.ieee.org/xsd/LOM#"/>
    <Prefix name="solo" IRI="http://example.org/solo-education-ontology#"/>

    <!-- Ontology Metadata -->
    <Annotation>
        <AnnotationProperty abbreviatedIRI="dc:title"/>
        <Literal>SOLO Taxonomy Educational Ontology</Literal>
    </Annotation>
    <Annotation>
        <AnnotationProperty abbreviatedIRI="dc:description"/>
        <Literal>A seed ontology for representing educational content, SOLO taxonomy levels, learning objects, and assessment questions. This ontology evolves as the system processes new educational materials.</Literal>
    </Annotation>
    <Annotation>
        <AnnotationProperty abbreviatedIRI="dc:creator"/>
        <Literal>SOLO Quiz Generator Project</Literal>
    </Annotation>

    <!-- ================================================================== -->
    <!-- CORE CLASS HIERARCHY                                               -->
    <!-- ================================================================== -->

    <!-- Top-level Educational Entity -->
    <Declaration>
        <Class IRI="#EducationalEntity"/>
    </Declaration>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#EducationalEntity</IRI>
        <Literal>Educational Entity</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#EducationalEntity</IRI>
        <Literal>Abstract base class for all educational entities in the ontology</Literal>
    </AnnotationAssertion>

    <!-- ==================== COURSE STRUCTURE ==================== -->
    
    <!-- Course -->
    <Declaration>
        <Class IRI="#Course"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Course"/>
        <Class IRI="#EducationalEntity"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Course</IRI>
        <Literal>Course</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Course</IRI>
        <Literal>A structured program of study on a particular subject (e.g., Operating Systems, Biology)</Literal>
    </AnnotationAssertion>

    <!-- Lesson -->
    <Declaration>
        <Class IRI="#Lesson"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Lesson"/>
        <Class IRI="#EducationalEntity"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Lesson</IRI>
        <Literal>Lesson</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Lesson</IRI>
        <Literal>A single unit of instruction within a course, typically corresponding to one PDF or document</Literal>
    </AnnotationAssertion>

    <!-- Section -->
    <Declaration>
        <Class IRI="#Section"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Section"/>
        <Class IRI="#EducationalEntity"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Section</IRI>
        <Literal>Section</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Section</IRI>
        <Literal>A major topic division within a lesson</Literal>
    </AnnotationAssertion>

    <!-- ==================== LEARNING OBJECTS (IEEE LOM inspired) ==================== -->
    
    <!-- LearningObject - Base class -->
    <Declaration>
        <Class IRI="#LearningObject"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#LearningObject"/>
        <Class IRI="#EducationalEntity"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#LearningObject</IRI>
        <Literal>Learning Object</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#LearningObject</IRI>
        <Literal>The smallest meaningful unit of educational content that can be independently understood and assessed. Inspired by IEEE LOM standard.</Literal>
    </AnnotationAssertion>

    <!-- Learning Object Types -->
    <Declaration>
        <Class IRI="#Concept"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Concept"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Concept</IRI>
        <Literal>Concept</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Concept</IRI>
        <Literal>An abstract idea or general notion (e.g., 'Process Scheduling', 'Photosynthesis')</Literal>
    </AnnotationAssertion>

    <Declaration>
        <Class IRI="#Definition"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Definition"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Definition</IRI>
        <Literal>Definition</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Definition</IRI>
        <Literal>A formal explanation of the meaning of a term or concept</Literal>
    </AnnotationAssertion>

    <Declaration>
        <Class IRI="#Procedure"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Procedure"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Procedure</IRI>
        <Literal>Procedure</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Procedure</IRI>
        <Literal>A step-by-step process or algorithm</Literal>
    </AnnotationAssertion>

    <Declaration>
        <Class IRI="#Principle"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Principle"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Principle</IRI>
        <Literal>Principle</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Principle</IRI>
        <Literal>A fundamental truth, law, or rule that guides behavior or explains phenomena</Literal>
    </AnnotationAssertion>

    <Declaration>
        <Class IRI="#Example"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Example"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Example</IRI>
        <Literal>Example</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Example</IRI>
        <Literal>A concrete instance that illustrates a concept or principle</Literal>
    </AnnotationAssertion>

    <Declaration>
        <Class IRI="#Fact"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#Fact"/>
        <Class IRI="#LearningObject"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Fact</IRI>
        <Literal>Fact</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Fact</IRI>
        <Literal>A verified piece of information that is objectively true</Literal>
    </AnnotationAssertion>

    <!-- ==================== SOLO TAXONOMY LEVELS ==================== -->
    
    <Declaration>
        <Class IRI="#SoloLevel"/>
    </Declaration>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#SoloLevel</IRI>
        <Literal>SOLO Taxonomy Level</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#SoloLevel</IRI>
        <Literal>Structure of Observed Learning Outcome (SOLO) taxonomy level, as defined by Biggs and Collis (1982). Describes the complexity of a learner's understanding.</Literal>
    </AnnotationAssertion>

    <!-- SOLO Level Individuals -->
    <Declaration>
        <NamedIndividual IRI="#Prestructural"/>
    </Declaration>
    <ClassAssertion>
        <Class IRI="#SoloLevel"/>
        <NamedIndividual IRI="#Prestructural"/>
    </ClassAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Prestructural</IRI>
        <Literal>Prestructural</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Prestructural</IRI>
        <Literal>Level 0: No understanding. Response shows no relevant knowledge.</Literal>
    </AnnotationAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#levelOrder"/>
        <NamedIndividual IRI="#Prestructural"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">0</Literal>
    </DataPropertyAssertion>

    <Declaration>
        <NamedIndividual IRI="#Unistructural"/>
    </Declaration>
    <ClassAssertion>
        <Class IRI="#SoloLevel"/>
        <NamedIndividual IRI="#Unistructural"/>
    </ClassAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Unistructural</IRI>
        <Literal>Unistructural</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Unistructural</IRI>
        <Literal>Level 1: Understanding of ONE aspect. Can identify, name, or follow simple procedures.</Literal>
    </AnnotationAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#levelOrder"/>
        <NamedIndividual IRI="#Unistructural"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">1</Literal>
    </DataPropertyAssertion>

    <Declaration>
        <NamedIndividual IRI="#Multistructural"/>
    </Declaration>
    <ClassAssertion>
        <Class IRI="#SoloLevel"/>
        <NamedIndividual IRI="#Multistructural"/>
    </ClassAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Multistructural</IRI>
        <Literal>Multistructural</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Multistructural</IRI>
        <Literal>Level 2: Understanding of SEVERAL aspects independently. Can describe, list, combine.</Literal>
    </AnnotationAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#levelOrder"/>
        <NamedIndividual IRI="#Multistructural"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">2</Literal>
    </DataPropertyAssertion>

    <Declaration>
        <NamedIndividual IRI="#Relational"/>
    </Declaration>
    <ClassAssertion>
        <Class IRI="#SoloLevel"/>
        <NamedIndividual IRI="#Relational"/>
    </ClassAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Relational</IRI>
        <Literal>Relational</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Relational</IRI>
        <Literal>Level 3: Understanding of RELATIONSHIPS between aspects. Can compare, contrast, analyze, explain causes.</Literal>
    </AnnotationAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#levelOrder"/>
        <NamedIndividual IRI="#Relational"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">3</Literal>
    </DataPropertyAssertion>

    <Declaration>
        <NamedIndividual IRI="#ExtendedAbstract"/>
    </Declaration>
    <ClassAssertion>
        <Class IRI="#SoloLevel"/>
        <NamedIndividual IRI="#ExtendedAbstract"/>
    </ClassAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#ExtendedAbstract</IRI>
        <Literal>Extended Abstract</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#ExtendedAbstract</IRI>
        <Literal>Level 4: TRANSFER to new domains. Can generalize, hypothesize, theorize, create new applications.</Literal>
    </AnnotationAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#levelOrder"/>
        <NamedIndividual IRI="#ExtendedAbstract"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">4</Literal>
    </DataPropertyAssertion>

